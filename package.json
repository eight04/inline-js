{
  "name": "inline-js",
  "version": "0.8.0",
  "description": "Static assets inliner",
  "author": "eight04 <eight04@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.js --cache",
    "nyc": "nyc --reporter=html mocha",
    "coverage": "nyc report --reporter=text-lcov | npx coveralls",
    "test": "npm run lint && npm run nyc",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push --follow-tags && npm publish",
    "build": "node cli.js README.md -o README.md",
    "build-doc": "rollup -c",
    "prepack": "crlf --set=LF cli.js"
  },
  "bin": {
    "inlinejs": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "main": "index.js",
  "eslintIgnore": [
    "coverage",
    "docs"
  ],
  "dependencies": {
    "config-locator": "^0.1.0",
    "fs-extra": "^8.0.1",
    "inline-js-core": "^0.5.0",
    "inline-js-default-resources": "^0.1.0",
    "inline-js-default-transforms": "^0.1.1",
    "neodoc": "^2.0.2",
    "treeify": "^1.1.0"
  },
  "devDependencies": {
    "crlf": "^1.1.1",
    "data-uri-to-buffer": "^2.0.1",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rollup": "^1.14.2",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-cjs-es": "^0.8.0",
    "rollup-plugin-copied": "0.0.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "sinon": "^7.3.2",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": "eight04/inline-js"
}
